record(calc,"${P}${AXIS_NAME}-SeqState") {
  field(DESC, "${AXIS_NAME}: Seq state")
  field(PINI, "$(PINI=1)")
  field(CALC,"A>>20 & 0xF")
  field(INPA,"${P}${AXIS_NAME}-Stat.VAL")
  field(FLNK, "${P}${AXIS_NAME}-LastIlock.PROC")
}


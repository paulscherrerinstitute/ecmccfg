#ETHERCAT
record(ai,"$(P)ec$(MASTER_ID)-alstates"){
  field(PINI, "1")
  field(DTYP, "asynInt32")
  field(INP,  "@asyn($(PORT),$(ADDR),$(TIMEOUT))ec$(MASTER_ID).alstates")
  field(PREC, "3")
  field(SCAN, "I/O Intr")
}

